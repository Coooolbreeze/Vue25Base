    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "npm run lint-fix",
    "lint": "eslint --ext .js --ext .jsx --ext .vue src/",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue src/",
    "clean": "rimraf dist && rimraf server-build",
    "upload": "node build/upload.js",
    "dev": "npm run lint-fix && concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.client.config.js",
    "dev:server": "nodemon server/main.js",
    "build": "npm run lint-fix && npm run clean && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js",
    "start": "cross-env NODE_ENV=production node server/main.js"
  },
